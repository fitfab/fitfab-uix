{
  "name": "fitfab-uix",
  "description": "fitfab user interface experience is a collection of React components and utilities for building user interfaces using web api technologies.",
  "homepage": "https://github.com/fitfab/fitfab-uix#readme",
  "private": false,
  "version": "1.2.0",
  "keywords": [
    "vitejs",
    "reactjs",
    "typescript",
    "ts-standard",
    "tailwindcss",
    "storybook"
  ],
  "author": "Miguel Julio <mjulio.developer@gmail.com> (http://fitfab.com)",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/fitfab-uix.umd.js",
  "module": "./dist/fitfab-uix.es.js",
  "exports": {
    ".": {
      "import": "./dist/fitfab-uix.es.js",
      "require": "./dist/fitfab-uix.umd.js"
    }
  },
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build": "yarn lint && tsc --noemit && vite build",
    "postbuild": "tsc --emitDeclarationOnly",
    "build:sb": "build-storybook",
    "preview": "vite preview",
    "lint": "ts-standard --fix",
    "prepare": "husky install",
    "test": "jest",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@storybook/addon-a11y": "^6.5.3",
    "@storybook/addon-actions": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-interactions": "^6.5.3",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/builder-vite": "^0.1.35",
    "@storybook/react": "^6.5.3",
    "@storybook/testing-library": "^0.0.11",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^28.0.3",
    "ts-standard": "^11.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "peerDependencies": {
    "@tailwindcss/line-clamp": "^0.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.24"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint"
    ]
  }
}
