{
  "name": "fitfab-uix",
  "description": "fitfab user interface experience is a collection of React components and utilities for building user interfaces using web api technologies.",
  "homepage": "https://github.com/fitfab/fitfab-uix#readme",
  "private": false,
  "version": "0.0.3",
  "keywords": [
    "vitejs",
    "reactjs",
    "typescript",
    "ts-standard",
    "tailwindcss",
    "storybook"
  ],
  "author": "Miguel Julio <mjulio.developer@gmail.com> (http://fitfab.com)",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/fitfab-uix.umd.js",
  "module": "./dist/fitfab-uix.es.js",
  "exports": {
    ".": {
      "import": "./dist/fitfab-uix.es.js",
      "require": "./dist/fitfab-uix.umd.js"
    }
  },
  "scripts": {
    "build": "yarn lint && tsc --noemit && vite build",
    "postbuild": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "dev": "start-storybook -p 6006",
    "build:sb": "build-storybook",
    "lint": "ts-standard --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.33",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "postcss": "^8.4.13",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.24",
    "ts-standard": "^11.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.7"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  }
}
